
public class BubbleSort {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
		System.out.println(test());

	}
	public static int[] bubbleSort(int[] arr) {
		int j=0;
		do {
		for (int i=0; i<arr.length;i++) {
			int a=arr[i];
			
			if(i<arr.length-1) {
				int b=arr[i+1];
			if (a>b) {
				arr[i+1]=a;
				arr[i]=b;;
						
			}
		} 
		}j++;
		}while (j<arr.length);
		
		return arr;
	}
	
	
		public static boolean test(){
			
			int[] arr1= {1,4,2,3,5};
			int[] arr2= {1,3,2,4,5};
			int[] arr3= {4,1,2,3,5};
			int[] arr4= {1,5,2,3,4};
			int[] arr5= {3,1,4,2,5};
			int[] arr6= {1,2,3,4,5};
			
			bubbleSort(arr2);
			bubbleSort(arr3);
			bubbleSort(arr4);
			arr5=bubbleSort(arr5);
			
			boolean out=false;
			
			if(arr1.equals(arr6)) {
				System.out.println("In 1");
				if(arr2==arr6) {
					System.out.println("In 2");
					if(arr3==arr6) {
						System.out.println("In 3");
						if(arr4==arr6) {
							System.out.println("In 4");
							if(arr5==arr6) {
								System.out.println("In 5");
							out=true;	
							}
							
						}
						
					}
					
					
				}
				
			}
			return out;
		}
	}


