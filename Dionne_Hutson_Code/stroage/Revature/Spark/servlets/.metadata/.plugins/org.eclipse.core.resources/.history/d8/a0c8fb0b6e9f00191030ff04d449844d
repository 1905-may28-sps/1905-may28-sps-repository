package src.com.revature.pojo;

public class ERS_USERS {
	private int userID;
	private String ERS_USERNAME;
	private String password;
	private String fn;
	private String ln;
	private String email;
	private int role;
	public ERS_USERS() {
	}
	public ERS_USERS(int userID, String ERS_USERNAME, String password, String fn, String ln, String email, int role) {
		super();
		this.userID = userID;
		this.ERS_USERNAME = ERS_USERNAME;
		this.password = password;
		this.fn = fn;
		this.ln = ln;
		this.email = email;
		this.role = role;
	}
	public int getUserID() {
		return userID;
	}
	public void setUserID(int userID) {
		this.userID = userID;
	}
	public String getUn() {
		return ERS_USERNAME;
	}
	public void setUn(String ERS_USERNAME) {
		this.ERS_USERNAME =ERS_USERNAME;
	}
	public String getPass() {
		return password;
	}
	public void setPass(String password) {
		this.password = password;
	}
	public String getFn() {
		return fn;
	}
	public void setFn(String fn) {
		this.fn = fn;
	}
	public String getLn() {
		return ln;
	}
	public void setLn(String ln) {
		this.ln = ln;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public int getRole() {
		return role;
	}
	public void setRole(int role) {
		this.role = role;
	}
	@Override
	public String toString() {
		return "ERS_USERS [userID=" + userID + ",username=" + ERS_USERNAME + ", password=" + password + ", fn=" + fn + ", ln=" + ln
				+ ", email=" + email + ", role=" + role + "]";
	}
	
	
	
}
